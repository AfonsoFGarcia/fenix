  	locking="false"
  />
   <field-descriptor
      name="keyInsuranceValue"
      column="KEY_INSURANCE_VALUE"
      jdbc-type="INTEGER"
   />
   <field-descriptor 
      name="year"
      column="YEAR"
      jdbc-type="VARCHAR"
   />       
   <field-descriptor
      name="state"
      column="STATE"
      conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaPeriodState2SqlPeriodState"
      jdbc-type="VARCHAR"/>       
   <field-descriptor
      name="beginDate"
      column="BEGIN_DATE"
      jdbc-type="DATE"
      conversion="org.apache.ojb.broker.accesslayer.conversions.JavaDate2SqlDateFieldConversion"
   />
   <field-descriptor
      name="endDate"
      column="END_DATE"
      jdbc-type="DATE"
      conversion="org.apache.ojb.broker.accesslayer.conversions.JavaDate2SqlDateFieldConversion"
    />
    
    
    
    
    
  
   
    
  
  
</class-descriptor> 
