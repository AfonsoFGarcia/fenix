<!-- *************************************************************************************************************** -->
<!-- Definitions for net.sourceforge.fenixedu.domain.Degree -->
<!-- *************************************************************************************************************** -->
<class-descriptor class="net.sourceforge.fenixedu.domain.Degree" table="DEGREE">
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
	/>
	<field-descriptor
		name="sigla"
		column="CODE"
		jdbc-type="VARCHAR"
	/>
	<field-descriptor
		name="nome"
		column="NAME"
		jdbc-type="VARCHAR"
	/>
	<field-descriptor
		name="nameEn"
		column="NAME_EN"
		jdbc-type="VARCHAR"
	/>
	<field-descriptor
		name="tipoCurso"
		column="TYPE_DEGREE"
		jdbc-type="INTEGER"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaTipoCurso2SqlTipoCursoFieldConversion"
	/>
	<field-descriptor
		name="ackOptLock"
		column="ACK_OPT_LOCK"
		jdbc-type="INTEGER"
		locking="true"
	/>
	<field-descriptor
		name="concreteClassForDegreeCurricularPlans"
		column="CLASS_NAME_FOR_DEGREE_CURRICULAR_PLANS"
		jdbc-type="VARCHAR"
	/>
	<collection-descriptor
			name="degreeCurricularPlans"
			element-class-ref="net.sourceforge.fenixedu.domain.DegreeCurricularPlan"
			proxy="true">
		<inverse-foreignkey field-ref="degreeKey"/>
	</collection-descriptor>
	<collection-descriptor
			name="degreeInfos"
			element-class-ref="net.sourceforge.fenixedu.domain.DegreeInfo"
			proxy="true">
		<inverse-foreignkey field-ref="degreeKey"/>
	</collection-descriptor>    
</class-descriptor>
