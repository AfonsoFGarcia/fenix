

<!-- *************************************************************************************************************** -->
<!-- Definitions for Dominio.InquiryAnswer -->
<!-- *************************************************************************************************************** -->
<class-descriptor class="Dominio.student.SchoolRegistrationInquiryAnswer" table="INQUIRY_ANSWER">
  <field-descriptor
    name="idInternal"
    column="ID_INTERNAL"
    jdbc-type="INTEGER"
	primarykey="true"
	autoincrement="true"
  />
  <field-descriptor
    name="keyStudent"
    column="KEY_STUDENT"
    jdbc-type="INTEGER"
  />
  <field-descriptor
    name="answer1"
    column="ANSWER1"
    jdbc-type="BIT"
  />
  <field-descriptor
    name="answer2"
    column="ANSWER2"
    jdbc-type="BIT"
  />
  <field-descriptor
    name="answer3"
    column="ANSWER3"
    jdbc-type="BIT"
  />
  <field-descriptor
    name="answer4"
    column="ANSWER4"
    jdbc-type="BIT"
  />
  <field-descriptor
    name="answer5"
    column="ANSWER5"
    jdbc-type="BIT"
  />
  <field-descriptor
    name="answer6"
    column="ANSWER6"
    jdbc-type="BIT"
  />
  <field-descriptor
    name="answer7"
    column="ANSWER7"
    jdbc-type="BIT"
  />
  <field-descriptor
    name="ackOptLock"
    column="ACK_OPT_LOCK"
    jdbc-type="INTEGER"
	locking="true"
  />

  <reference-descriptor
	name="student"
	class-ref="Dominio.Student"
	proxy="false">
	<foreignkey field-ref="keyStudent"/>
  </reference-descriptor>
</class-descriptor>



