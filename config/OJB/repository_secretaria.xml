 <class-descriptor
         class="Dominio.ExecutionYear"
         table="EXECUTION_YEAR"
    >
       <field-descriptor id="1"
          name="internalCode"
          column="ID_INTERNAL"
          jdbc-type="INTEGER"
          primarykey="true"
          autoincrement="true"
       />
       <field-descriptor id="2"
          name="year"
          column="YEAR"
          jdbc-type="VARCHAR"
       />
    </class-descriptor> 
