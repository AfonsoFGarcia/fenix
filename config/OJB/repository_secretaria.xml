<!-- ******************************************************************************************************************************* -->
<!-- Definitions for Dominio.StudentCurricularPlan -->
<!-- ******************************************************************************************************************************* -->
<class-descriptor class="Dominio.StudentCurricularPlan"
                  table="STUDENT_CURRICULAR_PLAN">
    <field-descriptor id="1"
        name="internalCode"
        column="ID_INTERNAL"
        jdbc-type="INTEGER"
        primarykey="true"
        autoincrement="true"
    />
    <field-descriptor id="2"
        name="studentKey"
        column="KEY_STUDENT"
        jdbc-type="INTEGER"
    />
    <field-descriptor id="3"
        name="courseCurricularPlanKey"
        column="KEY_COURSE_CURRICULAR_PLAN"
        jdbc-type="INTEGER"
    />
    <field-descriptor id="4"
        name="currentState"
        column="CURRENT_STATE"
        jdbc-type="INTEGER"
        conversion="ServidorPersistente.Conversores.JavaStudentCurricularPlanState2SqlStudentCurricularPlanStateFieldConversion"
    />
   <field-descriptor id="5"
        name="startDate"
        column="START_DATE"
        jdbc-type="DATE"
    />

    <reference-descriptor
         name="student"
         class-ref="Dominio.Student"
         proxy="true"
    >
         <foreignkey field-id-ref="2"/>
    </reference-descriptor>

    <reference-descriptor
         name="courseCurricularPlan"
         class-ref="Dominio.PlanoCurricularCurso"
         proxy="false"
    >
         <foreignkey field-id-ref="3"/>
    </reference-descriptor>

</class-descriptor>