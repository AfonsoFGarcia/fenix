<!-- Definitions for class QUALIFICATION -->

<class-descriptor class="net.sourceforge.fenixedu.domain.Qualification"
        table="QUALIFICATION">
              
        <field-descriptor 
           name="idInternal"    
           column="ID_INTERNAL"  
           jdbc-type="INTEGER"
           primarykey="true"
           autoincrement="true"
        />
        <field-descriptor
            name="mark"
            column="MARK"
            jdbc-type="VARCHAR"
         />
         <field-descriptor 
            name="school"
            column="SCHOOL"
            jdbc-type="VARCHAR"
         />
         <field-descriptor 
            name="title"
            column="TITLE"
            jdbc-type="VARCHAR"
         />
         <field-descriptor
         	name="degree"
         	column="DEGREE"
         	jdbc-type="VARCHAR"
         />
         <field-descriptor 
            name="personKey"
            column="KEY_PERSON"
            jdbc-type="INTEGER"
         />         
        <field-descriptor 
            name="date"
            column="DATE"
            jdbc-type="DATE"
         />         
		<field-descriptor 
            name="branch"
            column="BRANCH"
            jdbc-type="VARCHAR"
         />
		<field-descriptor 
            name="specializationArea"
            column="SPECIALIZATION_AREA"
            jdbc-type="VARCHAR"
         />
		<field-descriptor 
            name="degreeRecognition"
            column="DEGREE_RECOGNITION"
            jdbc-type="VARCHAR"
         />
		<field-descriptor 
            name="equivalenceDate"
            column="EQUIVALENCE_DATE"
            jdbc-type="DATE"
         />
		<field-descriptor 
            name="equivalenceSchool"
            column="EQUIVALENCE_SCHOOL"
            jdbc-type="VARCHAR"
         />
		<field-descriptor 
            name="countryKey"
            column="KEY_COUNTRY"
            jdbc-type="INTEGER"
         />                  
         <field-descriptor name="lastModificationDate" 
			column="LAST_MODIFICATION_DATE" 
			jdbc-type="TIMESTAMP"
			conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.TimeStamp2DateFieldConversion"
    	 />
	     <field-descriptor
			  name="ackOptLock"
			  column="ACK_OPT_LOCK"
			  jdbc-type="INTEGER"
			  locking="false"
		 />		 
		 <field-descriptor 
            name="year"
            column="YEAR"
            jdbc-type="VARCHAR"
         />
         <field-descriptor 
            name="type"
            column="TYPE"
			jdbc-type="VARCHAR"
	   		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.QualificationType2SqlQualificationType"
         /> 
		  
	     <reference-descriptor auto-retrieve="false"
         	name="person"
         	class-ref="net.sourceforge.fenixedu.domain.Person">
    	      <foreignkey field-ref="personKey"/>
	      </reference-descriptor>
	      
	  	  <reference-descriptor auto-retrieve="false"
         	name="country"
         	class-ref="net.sourceforge.fenixedu.domain.Country">
    	      <foreignkey field-ref="countryKey"/>
     	  </reference-descriptor>   
     	                      
</class-descriptor>