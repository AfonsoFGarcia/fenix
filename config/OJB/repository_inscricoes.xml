		name="codigoInterno"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
	/>
	<field-descriptor
		name="nome"
		column="NAME"
		jdbc-type="VARCHAR"
