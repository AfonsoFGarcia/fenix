	<field-descriptor name="ackOptLock" column="ACK_OPT_LOCK" jdbc-type="INTEGER" locking="true"/>
    <reference-descriptor name="curricularCourse" class-ref="Dominio.CurricularCourse">
	    <foreignkey field-ref="keyCurricularCourse"/>
    </reference-descriptor>
    <collection-descriptor name="restrictions" element-class-ref="Dominio.Restriction" proxy="true">
