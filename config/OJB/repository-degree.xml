<class-descriptor class="net.sourceforge.fenixedu.domain.degree.finalProject.DegreeFinalProject"
		table="DEGREE_FINAL_PROJECT">
	  <field-descriptor  name="idInternal" column="ID_INTERNAL"  
							     jdbc-type="INTEGER"  primarykey="true" autoincrement="true"/>

	  <field-descriptor name="keyExecutionYear" column="KEY_EXECUTION_YEAR" jdbc-type="INTEGER" />

	  <field-descriptor  name="title" column="TITLE" jdbc-type="VARCHAR" />
      <field-descriptor
      	name="ackOptLock"
      	column="ACK_OPT_LOCK"
      	jdbc-type="INTEGER"
      	locking="false"
      />
	  <reference-descriptor auto-retrieve="false"
			name="executionYear"
			class-ref="net.sourceforge.fenixedu.domain.ExecutionYear">
		<foreignkey field-ref="keyExecutionYear"/>
	 </reference-descriptor>   
 </class-descriptor>  

<class-descriptor class="net.sourceforge.fenixedu.domain.degree.finalProject.DegreeFinalProjectOrientation"
		table="DEGREE_FINAL_PROJECT_ORIENTATION">
	  <field-descriptor  name="idInternal" column="ID_INTERNAL"  
							     jdbc-type="INTEGER"  primarykey="true" autoincrement="true"/>

	  <field-descriptor name="keyTeacher" column="KEY_TEACHER" jdbc-type="INTEGER" />

	  <field-descriptor   name="keyDegreeFinalProject"  column="KEY_DEGREE_FINAL_PROJECT" jdbc-type="INTEGER" />
	  
	  <field-descriptor  name="coOrientation" column="CO_ORIENTATION" jdbc-type="BIT" />

	  <field-descriptor  name="percentage" column="PERCENTAGE" jdbc-type="DOUBLE"/>
      <field-descriptor
      	name="ackOptLock"
      	column="ACK_OPT_LOCK"
      	jdbc-type="INTEGER"
      	locking="false"
      />
	  <reference-descriptor auto-retrieve="false"
			name="teacher"
			class-ref="net.sourceforge.fenixedu.domain.Teacher"
			>
		<foreignkey field-ref="keyTeacher"/>
	 </reference-descriptor>   
	  <reference-descriptor auto-retrieve="false"
			name="degreeFinalProject"
			class-ref="net.sourceforge.fenixedu.domain.degree.finalProject.DegreeFinalProject"
			>
		<foreignkey field-ref="keyDegreeFinalProject"/>
	 </reference-descriptor>
 </class-descriptor>
