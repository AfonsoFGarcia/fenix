<class-descriptor class="net.sourceforge.fenixedu.domain.research.event.EventParticipation"
	table="EVENT_PARTICIPATION">
	
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
	/>
	
	<field-descriptor
		name="keyEvent" 
		column="KEY_EVENT"
		jdbc-type="INTEGER"
	/>
	
	<field-descriptor
		name="keyParty" 
		column="KEY_PARTY"
		jdbc-type="INTEGER"
	/>
	
	<field-descriptor
	   name="role"    
	   column="ROLE"  
	   jdbc-type="VARCHAR"
	   conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.EventParticipationRole2SqlEventParticipationRoleConversion"
    /> 
    
    <field-descriptor
        name="keyRootDomainObject"
        column="KEY_ROOT_DOMAIN_OBJECT"
        jdbc-type="INTEGER"
    />
    
    <reference-descriptor auto-retrieve="false" 
		name="event"
		class-ref="net.sourceforge.fenixedu.domain.research.event.Event"
		>
	    <foreignkey field-ref="keyEvent"/>
    </reference-descriptor>

    <reference-descriptor auto-retrieve="false" 
		name="party"
		class-ref="net.sourceforge.fenixedu.domain.organizationalStructure.Party"
		>
	    <foreignkey field-ref="keyParty"/>
    </reference-descriptor>
	
</class-descriptor>

