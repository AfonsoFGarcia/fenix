<class-descriptor class="net.sourceforge.fenixedu.domain.research.project.Project"
	table="PROJECT">
	
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
	/>
	
	<field-descriptor
		name="title"
		column="TITLE"
		jdbc-type="LONGVARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.MultiLanguageString2SqlMultiLanguageStringConversion"
	/>
	
	<field-descriptor
		name="projectAbstract"
		column="ABSTRACT"
		jdbc-type="LONGVARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.MultiLanguageString2SqlMultiLanguageStringConversion"
	/>

	<field-descriptor
		name="startDate"
		column="START_DATE"
		jdbc-type="VARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaYearMonth2SqlStringFieldConversion"
	/>
	
	<field-descriptor
		name="endDate"
		column="END_DATE"
		jdbc-type="VARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaYearMonth2SqlStringFieldConversion"
	/>
	
	<field-descriptor
	   name="projectType"    
	   column="TYPE"  
	   jdbc-type="VARCHAR"
	   conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.ProjectType2SqlProjectTypeConversion"
    /> 
	

</class-descriptor>

<class-descriptor class="net.sourceforge.fenixedu.domain.research.event.Event" table="RESEARCH_EVENT">
	
	<field-descriptor
         name="idInternal"
         column="ID_INTERNAL"
         jdbc-type="INTEGER"
         primarykey="true"
         autoincrement="true"
	/>
	
	<field-descriptor
		name="name"
		column="NAME"
		jdbc-type="LONGVARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.MultiLanguageString2SqlMultiLanguageStringConversion"
	/>
	
	<field-descriptor
		name="startDate"
		column="START_DATE"
		jdbc-type="VARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaYearMonthDay2SqlStringFieldConversion"
	/>
	
	<field-descriptor
		name="endDate"
		column="END_DATE"
		jdbc-type="VARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaYearMonthDay2SqlStringFieldConversion"
	/>
	
	<field-descriptor
		name="eventLocation"
		column="LOCATION"
		jdbc-type="VARCHAR"
	/>

	<field-descriptor
		name="fee"
		column="FEE"
		jdbc-type="BIT"
	/>

	<field-descriptor 
	   name="eventType"    
	   column="TYPE"  
	   jdbc-type="VARCHAR"
	   conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.ResearchEventType2SqlResearchEventTypeConversion"
    />
    
    <field-descriptor
        name="keyRootDomainObject"
        column="KEY_ROOT_DOMAIN_OBJECT"
        jdbc-type="INTEGER"
    />
    
	<collection-descriptor proxy="true" auto-retrieve="false" collection-class="org.apache.ojb.broker.util.collections.ManageableVector"
			name="projects"
			element-class-ref="net.sourceforge.fenixedu.domain.research.project.Project"
			indirection-table="PROJECT_EVENT"
			>
		<fk-pointing-to-this-class column="KEY_EVENT"/>
		<fk-pointing-to-element-class column="KEY_PROJECT"/>
	</collection-descriptor>
		
	<collection-descriptor proxy="true" auto-retrieve="false" collection-class="org.apache.ojb.broker.util.collections.ManageableVector"
         	name="eventParticipations"
         	element-class-ref="net.sourceforge.fenixedu.domain.research.event.EventParticipation">
         <inverse-foreignkey field-ref="keyEvent"/>
    </collection-descriptor>
</class-descriptor>


