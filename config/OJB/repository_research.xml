<class-descriptor class="net.sourceforge.fenixedu.domain.research.project.Project"
	table="RESEARCH_PROJECT">
	
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
	/>

	<field-descriptor
		name="number"
		column="NUMBER"
		jdbc-type="INTEGER"
	/>

	<field-descriptor
	    name="acronym"
	    column="ACRONYM"
	    jdbc-type="VARCHAR"
	/>
	
	<field-descriptor
		name="title"
		column="TITLE"
		jdbc-type="LONGVARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.MultiLanguageString2SqlMultiLanguageStringConversion"
	/>
	
	<field-descriptor
		name="projectAbstract"
		column="ABSTRACT"
		jdbc-type="LONGVARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.MultiLanguageString2SqlMultiLanguageStringConversion"
	/>

	<field-descriptor
		name="startDate"
		column="START_DATE"
		jdbc-type="VARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaYearMonth2SqlStringFieldConversion"
	/>
	
	<field-descriptor
		name="endDate"
		column="END_DATE"
		jdbc-type="VARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaYearMonth2SqlStringFieldConversion"
	/>
	
	<field-descriptor
	   name="projectType"    
	   column="TYPE"  
	   jdbc-type="VARCHAR"
	   conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.ProjectType2SqlProjectTypeConversion"
    /> 
	

</class-descriptor>

