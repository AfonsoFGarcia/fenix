
<!-- ******************************************************************************************************************************* -->
<!-- Definitions for net.sourceforge.fenixedu.domain.log.ServiceLog -->
<!-- ******************************************************************************************************************************* -->
<class-descriptor class="net.sourceforge.fenixedu.domain.log.ServiceLog" table="SERVICE_LOG">
	
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
        jdbc-type="INTEGER"
        primarykey="true"
        autoincrement="true"
    />
	
	<field-descriptor
	   	name="ackOptLock"
      	column="ACK_OPT_LOCK"
      	jdbc-type="INTEGER"
      	locking="true"
	/>
	
	<field-descriptor
	   	name="invocationDate"
      	column="INVOCATION_DATE"
		jdbc-type="TIMESTAMP"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.TimeStamp2DateFieldConversion"
	/>
			
	<field-descriptor 
	    name="invokerUsername"
	    column="INVOKER_USERNAME"
	    jdbc-type="VARCHAR"
	/>
			
	<field-descriptor 
	    name="serviceName"
	    column="SERVICE_NAME"
	    jdbc-type="VARCHAR"
	/>	
				
	<field-descriptor 
	    name="serviceArguments"
	    column="SERVICE_ARGUMENTS"
	    jdbc-type="LONGVARCHAR"
	/>
	
</class-descriptor>

