<!-- Definitions for Dominio.Coordinator -->
<class-descriptor class="Dominio.Campus" table="CAMPUS">
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
	/>	
	<field-descriptor
		name="name"
		column="NAME"
		jdbc-type="VARCHAR"
	/>
	<field-descriptor
		name="degreeKey"
		column="KEY_DEGREE"
		jdbc-type="INTEGER"
	/>	
	<reference-descriptor
				name="degree"
				class-ref="Dominio.Curso"
				proxy="true">
			<foreignkey field-ref="degreeKey"/>
	</reference-descriptor>     	   	
</class-descriptor>		
