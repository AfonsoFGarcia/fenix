<!-- Definitions for net.sourceforge.fenixedu.domain.curricularPeriod.CurricularPeriod -->
<!-- ******************************************************************************************************************************* -->
<class-descriptor class="net.sourceforge.fenixedu.domain.curricularPeriod.CurricularPeriod" table="CURRICULAR_PERIOD">

	<field-descriptor
        name="idInternal"
        column="ID_INTERNAL"
        jdbc-type="INTEGER"
        primarykey="true"
        autoincrement="true"
    />
    
    <field-descriptor
        name="keyParent"
        column="KEY_PARENT"
        jdbc-type="INTEGER"
    />      
    
    <field-descriptor
        name="keyDegreeCurricularPlan"
        column="KEY_DEGREE_CURRICULAR_PLAN"
        jdbc-type="INTEGER"
    />      
    
    <field-descriptor
        name="order"
        column="CHILD_ORDER"
        jdbc-type="INTEGER"
    />      

    <field-descriptor
        name="periodType"
        column="PERIOD_TYPE"
        jdbc-type="VARCHAR"
        conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaCurricularPeriodType2SqlCurricularPeriodTypeFieldConversion"
    />      

	<reference-descriptor auto-retrieve="false"
    	name="degreeCurricularPlan"
        class-ref="net.sourceforge.fenixedu.domain.DegreeCurricularPlan">
	    <foreignkey field-ref="keyDegreeCurricularPlan"/>
	</reference-descriptor>	

	<reference-descriptor auto-retrieve="false"
    	name="parent"
        class-ref="net.sourceforge.fenixedu.domain.curricularPeriod.CurricularPeriod">
	    <foreignkey field-ref="keyParent"/>
	</reference-descriptor>	

	<collection-descriptor proxy="true" auto-retrieve="false" collection-class="org.apache.ojb.broker.util.collections.ManageableVector"
			name="childs"
			element-class-ref="net.sourceforge.fenixedu.domain.curricularPeriod.CurricularPeriod">
		<inverse-foreignkey field-ref="keyParent"/>
	</collection-descriptor>

	<collection-descriptor proxy="true" auto-retrieve="false" collection-class="org.apache.ojb.broker.util.collections.ManageableVector"
			name="contexts"
			element-class-ref="net.sourceforge.fenixedu.domain.degreeStructure.Context">
		<inverse-foreignkey field-ref="keyCurricularPeriod"/>
	</collection-descriptor>
	
</class-descriptor>
