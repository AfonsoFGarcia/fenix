<!-- Definitions for . -->
<class-descriptor 
  class="Dominio.managementAssiduousness.MoneyCostCenter"
  table="MONEY_COST_CENTER"
>
	<field-descriptor
	    name="idInternal"
	    column="ID_INTERNAL"
	    jdbc-type="INTEGER"
	    primarykey="true"
	    autoincrement="true"
	/>
	<field-descriptor
	   	name="ackOptLock"
	  	column="ACK_OPT_LOCK"
	   	jdbc-type="INTEGER"
	   	locking="true"
	/>
	<field-descriptor
	    name="initialValue"
	    column="INITIAL_VALUE"
	    jdbc-type="DOUBLE"
	/>
	<field-descriptor
	    name="totalValue"
	    column="TOTAL_VALUE"
	    jdbc-type="DOUBLE"
	/>
	<field-descriptor
	    name="spentValue"
	    column="SPENT_VALUE"
	    jdbc-type="DOUBLE"
	/>
	<field-descriptor
	    name="year"
	    column="YEAR"
	    jdbc-type="INTEGER"
	/>
	<field-descriptor
	   	name="costCenterKey"
	   	column="KEY_COST_CENTER"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
	   	name="who"
	   	column="KEY_EMPLOYEE"
		jdbc-type="INTEGER"
	/>   	   	
	<field-descriptor
		name="when"
		column="WHEN_ALTER"
		jdbc-type="TIMESTAMP"
		conversion="ServidorPersistente.Conversores.TimeStamp2DateFieldConversion"
	/>   
  	<reference-descriptor
         name="costCenter"
         class-ref="Dominio.CostCenter"
         proxy="true">
         <foreignkey field-ref="costCenterKey"/>
  	</reference-descriptor>
    <reference-descriptor
      	name="whoEmployee"
       	class-ref="Dominio.Employee"
       	proxy="false"
		refresh="true">
        <foreignkey field-ref="who"/>
    </reference-descriptor>  
</class-descriptor>

