relation sendProposal {
	Dominio.GroupPropertiesExecutionCourse playsRole groupPropertiesExecutionCourse {
		multiplicity *;
	}
	Dominio.ExecutionCourse playsRole senderExecutionCourse;
}



relation goalExecutionCourse {
	Dominio.GroupPropertiesExecutionCourse playsRole groupPropertiesExecutionCourse {
		multiplicity *;
	}
	Dominio.ExecutionCourse playsRole executionCourse;
}


