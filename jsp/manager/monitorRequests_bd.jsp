<%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %><%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean" %><%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %><%@ taglib uri="/WEB-INF/taglibs-datetime.tld" prefix="dt" %><h2><bean:message key="manager.monitor.requests.title"/></h2><br /><html:form action="/monitorRequestLogs">	<htnl:hidden property="method" value="listFiles"/>	<html:text property="logDirName"/>	<html:submit styleClass="inputbutton">		<bean:message key="label.select"/>	</html:submit></html:form><br /><br /><logic:present name="fileNames">	<logic:iterate id="filename" name="fileNames">		<html:link page="<%= "/monitorRequestLogs.do?method=analyseFile&amp;logFileName="							  	+ pageContext.findAttribute("filename") %>">			<bean:write name="filename"/><br />		</html:link>	</logic:iterate></logic:present>