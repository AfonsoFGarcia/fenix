<%@ page language="java" %><%@ page import="org.apache.struts.action.Action" %><%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %><%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %><%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean" %><%@ page import="net.sourceforge.fenixedu.presentationTier.Action.sop.utils.SessionConstants" %><table width="100%" border="0" cellpadding="0" cellspacing="0">	<tr>    	<td class="infoselected">    		<p>O curso seleccionado &eacute;:</p>    		<strong><jsp:include page="context.jsp"/></strong>		</td>	</tr></table><br/><h2><bean:message key="title.choose.discipline"/></h2>        <span class="error"><html:errors /></span>        <html:form action="/chooseExecutionCourseForExams" focus="executionCourseID">                	<html:hidden property="page" value="1"/>		<html:hidden property="<%= SessionConstants.EXECUTION_PERIOD_OID %>"					 value="<%= pageContext.findAttribute("executionPeriodOID").toString() %>"/>		<html:hidden property="<%= SessionConstants.EXECUTION_DEGREE_OID %>"					 value="<%= pageContext.findAttribute("executionDegreeOID").toString() %>"/>		<html:hidden property="<%= SessionConstants.CURRICULAR_YEAR_OID %>"					 value="<%= pageContext.findAttribute("curricularYearOID").toString() %>"/>		<html:hidden property="<%= SessionConstants.DAY %>"					 value="<%= pageContext.findAttribute(SessionConstants.DAY).toString() %>"/>		<html:hidden property="<%= SessionConstants.MONTH %>"					 value="<%= pageContext.findAttribute(SessionConstants.MONTH).toString() %>"/>		<html:hidden property="<%= SessionConstants.YEAR %>"					 value="<%= pageContext.findAttribute(SessionConstants.YEAR).toString() %>"/>    	<table border="0" cellspacing="0" cellpadding="0">          <tr>            <td nowrap="nowrap" class="formTD">            <bean:message key="property.course"/>            </td>            <td nowrap="nowrap" class="formTD">			  <html:select property="executionCourseID" size="1">			  	 <option value=""></option>			     <html:options	property="idInternal" 			     				labelProperty="nome" 								collection="<%= SessionConstants.EXECUTION_COURSE_LIST_KEY %>" />			  </html:select>            </td>          </tr>        </table><br /><html:hidden property="method" value="choose"/><html:submit value="Selecionar" styleClass="inputbutton">	<bean:message key="label.next"/></html:submit></html:form>