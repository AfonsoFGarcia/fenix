<%@ page language="java" %><%@ page import="org.apache.struts.action.Action" %><%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %><%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %><%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean" %><%@ page import="ServidorApresentacao.Action.sop.utils.SessionConstants" %><h2><bean:message key="title.choose.discipline"/></h2>        <span class="error"><html:errors /></span>        <html:form action="/associateExecutionCourseToExam" focus="executionDegreeOID">        		<html:hidden property="<%= SessionConstants.EXECUTION_PERIOD_OID %>"					 value="<%= pageContext.findAttribute(SessionConstants.EXECUTION_PERIOD_OID).toString() %>"/>		<html:hidden property="<%= SessionConstants.EXECUTION_DEGREE_OID %>"					 value="<%= pageContext.findAttribute(SessionConstants.EXECUTION_DEGREE_OID).toString() %>"/>		<html:hidden property="<%= SessionConstants.CURRICULAR_YEAR_OID %>"					 value="<%= pageContext.findAttribute(SessionConstants.CURRICULAR_YEAR_OID).toString() %>"/>		<html:hidden property="<%= SessionConstants.EXECUTION_COURSE_OID %>"					 value="<%= pageContext.findAttribute("executionCourseOID").toString() %>"/>    	<table border="0" cellspacing="0" cellpadding="0">			<html:hidden property="method" value="choose"/><%--        	<html:hidden property="page" value="1"/> --%>	<logic:iterate indexId="i" id="executionCourseID" name="executionCoursesArray">			<html:hidden property="executionCourses" value="<%= pageContext.findAttribute("executionCourseID").toString() %>" />	</logic:iterate>	<html:hidden property="day" />	<html:hidden property="month" />	<html:hidden property="year" />	<html:hidden property="beginningHour" />	<html:hidden property="beginningMinute" />	<html:hidden property="endHour" />	<html:hidden property="endMinute" />	<html:hidden property="season" />		<logic:iterate indexId="i" id="scopeID" name="scopes">			<html:hidden property="scopes" value="<%= pageContext.findAttribute("scopeID").toString() %>" />	</logic:iterate>		<logic:iterate indexId="i" id="roomID" name="rooms">			<html:hidden property="rooms" value="<%= pageContext.findAttribute("roomID").toString() %>" />	</logic:iterate>	          <tr>            <td nowrap="nowrap" class="formTD">            <bean:message key="property.context.degree"/>            </td>            <td nowrap="nowrap" class="formTD">			  <html:select property="executionDegreeOID" size="1" onchange="this.form.method.value='listExecutionCourse';this.form.submit();">			     <html:options	property="value" 			     				labelProperty="label" 								collection="<%= SessionConstants.DEGREES %>" />			  </html:select>            </td>          </tr>          <tr>            <td nowrap="nowrap" class="formTD">            <bean:message key="property.context.curricular.year"/>            </td>            <td nowrap="nowrap" class="formTD">			  <html:select property="curricularYear" size="1" onchange="this.form.method.value='listExecutionCourse';this.form.submit();">			     <html:options	property="value" 			     				labelProperty="label" 								collection="<%= SessionConstants.LABELLIST_CURRICULAR_YEARS %>" />			  </html:select>            </td>          </tr>		<logic:notEmpty name="<%= SessionConstants.EXECUTION_COURSE_LIST_KEY %>">          <tr>            <td nowrap="nowrap" class="formTD">            <bean:message key="property.course"/>            </td>            <td nowrap="nowrap" class="formTD">        	<html:hidden property="page" value="1"/>			  <html:select property="executionCourses" size="1" value="">			  		<html:option value=""></html:option>				    <html:options	property="idInternal" 				     				labelProperty="nome" 									collection="<%= SessionConstants.EXECUTION_COURSE_LIST_KEY %>" />			  </html:select>            </td>          </tr>          </logic:notEmpty>        </table><br /><logic:present name="<%= SessionConstants.EXAM_OID %>">	<html:hidden property="<%= SessionConstants.EXAM_OID %>"		value="<%= pageContext.findAttribute(SessionConstants.EXAM_OID).toString() %>"/></logic:present><logic:notEmpty name="<%= SessionConstants.EXECUTION_COURSE_LIST_KEY %>"><html:submit value="Selecionar" styleClass="inputbutton">	<bean:message key="label.next"/></html:submit></logic:notEmpty></html:form>