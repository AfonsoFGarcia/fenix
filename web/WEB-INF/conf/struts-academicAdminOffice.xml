<?xml version="1.0" encoding="UTF-8"?>
<struts-config>
      <!-- ========== Data Source Configuration =============================== -->
      <!-- ========== Form Bean Definitions ================================== -->
      <form-beans type="org.apache.struts.action.ActionFormBean">
            <!-- VIEW STUDENT CURRICULUM FORMS -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="studentCurricularPlanAndEnrollmentsSelectionForm">
                  <form-property type="java.lang.String" name="studentCPID"></form-property>
                  <form-property type="java.lang.String" name="select"></form-property>
                  <form-property type="java.lang.String" name="studentNumber"></form-property>
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
                  <form-property initial="0" type="java.lang.Integer" name="degreeCurricularPlanID"></form-property>
                  <form-property type="java.lang.String" name="organizedBy"></form-property>
                  <form-property initial="false" type="java.lang.Boolean" name="detailed"></form-property>
                  <form-property type="java.lang.String" name="viewType"></form-property>
            </form-bean>
            <!-- Bean for The Creation of a Contributor -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="createContributorForm">
                  <form-property type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="contributorType"></form-property>
                  <form-property type="java.lang.String" name="contributorNumber"></form-property>
                  <form-property type="java.lang.String" name="contributorName"></form-property>
                  <form-property type="java.lang.String" name="contributorAddress"></form-property>
                  <form-property type="java.lang.String" name="areaCode"></form-property>
                  <form-property type="java.lang.String" name="areaOfAreaCode"></form-property>
                  <form-property type="java.lang.String" name="area"></form-property>
                  <form-property type="java.lang.String" name="parishOfResidence"></form-property>
                  <form-property type="java.lang.String" name="districtSubdivisionOfResidence"></form-property>
                  <form-property type="java.lang.String" name="districtOfResidence"></form-property>
                  <form-property type="java.lang.Integer" name="contributorId"></form-property>
            </form-bean>
            <!-- Bean for The Choosing of a Contributor -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="chooseContributorForm">
                  <form-property type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="contributorNumber"></form-property>
            </form-bean>
            <!-- bolonhaStudentEnrollmentForm -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="bolonhaStudentEnrollmentForm">
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.Boolean" name="withRules"></form-property>
            </form-bean>
            <!-- Bean for Student Dismissal -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="studentDismissalForm">
                  <form-property type="java.lang.String[]" name="creditsToDelete"></form-property>
            </form-bean>
            <!-- Beans for MarkSheet Management -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="markSheetManagementForm">
                  <form-property type="java.lang.Integer" name="epID"></form-property>
                  <form-property type="java.lang.Integer" name="dID"></form-property>
                  <form-property type="java.lang.Integer" name="dcpID"></form-property>
                  <form-property type="java.lang.Integer" name="ccID"></form-property>
                  <form-property type="java.lang.Integer" name="msID"></form-property>
                  <form-property type="java.lang.String" name="tn"></form-property>
                  <form-property type="java.lang.String" name="ed"></form-property>
                  <form-property type="java.lang.String" name="mss"></form-property>
                  <form-property type="java.lang.String" name="mst"></form-property>
                  <form-property type="java.lang.Integer" name="evaluationID"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="printMarkSheetForm">
                  <form-property type="java.lang.String" name="markSheet"></form-property>
                  <form-property type="java.lang.String" name="ecID"></form-property>
                  <form-property type="java.lang.String" name="dcpID"></form-property>
                  <form-property type="java.lang.String" name="printerName"></form-property>
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="markSheetSendMailForm">
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.Integer[]" name="markSheetIDs"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="executionYearDegreeCurricularPlanForm">
                  <form-property type="java.lang.String" name="executionYearID"></form-property>
                  <form-property type="java.lang.String" name="degreeCurricularPlanID"></form-property>
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
            </form-bean>
            <!-- Bean for Student Dismissal -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="studentExternalEnrolmentsForm">
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.String" name="studentId"></form-property>
                  <form-property type="java.lang.String" name="externalUnitId"></form-property>
                  <form-property type="java.lang.String[]" name="selectedExternalCurricularCourses"></form-property>
                  <form-property type="java.lang.String[]" name="externalEnrolmentsToDelete"></form-property>
            </form-bean>
            <!-- Bean for Student Dismissal -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="notNeedToEnrolForm">
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.Integer[]" name="notNeedToEnrolEnrolments"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="deleteregistrationActualInfoForm">
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.String" name="registrationId"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="externalUnitsForm">
                  <form-property type="java.lang.String" name="method"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="candidacyForm">
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.String[]" name="degreesToDelete"></form-property>
            </form-bean>
      </form-beans>
      <!-- ========== Global Exception Definitions ============================== -->
      <global-exceptions>
            <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixContainerExceptionHandler" key="error.Exception" type="java.lang.Exception"></exception>
            <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixContainerExceptionHandler" key="error.NotAuthorized" type="net.sourceforge.fenixedu.applicationTier.Servico.exceptions.NotAuthorizedException"></exception>
            <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixContainerExceptionHandler" key="error.NotAuthorized" type="net.sourceforge.fenixedu.injectionCode.IllegalDataAccessException"></exception>
      </global-exceptions>
      <!-- ========== Global Forward Definitions =============================== -->
      <global-forwards type="org.apache.struts.action.ActionForward"></global-forwards>
      <!-- ========== Action Mapping Definitions =============================== -->
      <action-mappings type="org.apache.struts.action.ActionMapping">
            <action scope="request" name="studentCurricularPlanAndEnrollmentsSelectionForm" parameter="method" type="net.sourceforge.fenixedu.presentationTier.Action.commons.student.CurriculumDispatchAction" path="/viewCurriculum">
                  <forward path="student.showStudentCurriculum" name="ShowStudentCurriculum"></forward>
                  <forward path="df.page.showStudentCurricularPlans" name="ShowStudentCurricularPlans"></forward>
                  <forward path="df.page.showStudentCurriculumForCoordinator" name="ShowStudentCurriculumForCoordinator"></forward>
                  <forward path="df.page.notAuthorized" name="NotAuthorized"></forward>
            </action>
            <action scope="request" name="studentCurricularPlanAndEnrollmentsSelectionForm" parameter="method" type="net.sourceforge.fenixedu.presentationTier.Action.commons.student.CurriculumDispatchAction" path="/viewStudentCurriculum">
                  <forward path="student.showStudentCurriculum" name="ShowStudentCurriculum"></forward>
                  <forward path="df.page.showStudentCurricularPlans" name="ShowStudentCurricularPlans"></forward>
                  <forward path="df.page.showStudentCurriculumForCoordinator" name="ShowStudentCurriculumForCoordinator"></forward>
                  <forward path="df.page.notAuthorized" name="NotAuthorized"></forward>
            </action>
            <!-- Dispatch Action to Create the Contributor Information -->
            <!-- action to visualize Contributors -->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.masterDegree.administrativeOffice.contributor.ListContributorsDispatchAction" scope="request" parameter="method" path="/visualizeContributors" name="chooseContributorForm">
                  <forward path="contributor.chooseContributor" name="PrepareReady"></forward>
                  <forward path="contributor.visualizeContributor" name="ActionReady"></forward>
                  <forward path="contributor.selectContributorFromList" name="ChooseContributor"></forward>
            </action>
            <!-- action to edit Contributors -->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.masterDegree.administrativeOffice.contributor.ListContributorsDispatchAction" scope="request" parameter="method" path="/editContributors" name="chooseContributorForm">
                  <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixErrorExceptionHandler" key="resources.Action.exceptions.ExistingActionException" type="net.sourceforge.fenixedu.presentationTier.Action.exceptions.ExistingActionException"></exception>
                  <forward path="contributor.chooseContributor" name="PrepareReady"></forward>
                  <forward path="/editContributor.do?method=prepareEdit&amp;page=0" name="ActionReady"></forward>
                  <forward path="contributor.selectContributorFromList" name="ChooseContributor"></forward>
            </action>
            <!-- Action to Edit ONE contributor -->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.masterDegree.administrativeOffice.contributor.ListContributorsDispatchAction" scope="request" parameter="method" path="/editContributor" name="createContributorForm">
                  <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixErrorExceptionHandler" key="resources.Action.exceptions.ExistingActionException" type="net.sourceforge.fenixedu.presentationTier.Action.exceptions.ExistingActionException"></exception>
                  <forward path="contributor.editContributor" name="EditReady"></forward>
                  <forward path="contributor.editContributorSuccess" name="EditSuccess"></forward>
            </action>
            <!-- ==================== MarkSheet Management ================== -->
            <action scope="request" name="markSheetManagementForm" parameter="method" input="show.markSheetManagement.create.page.step1" type="net.sourceforge.fenixedu.presentationTier.Action.administrativeOffice.gradeSubmission.MarkSheetCreateDispatchAction" path="/createMarkSheet">
                  <forward path="show.markSheetManagement.create.page.step1" name="createMarkSheetStep1"></forward>
                  <forward path="show.markSheetManagement.create.page.step2" name="createMarkSheetStep2"></forward>
                  <forward path="/markSheetManagement.do?method=prepareSearchMarkSheetFilled" name="searchMarkSheetFilled"></forward>
                  <forward path="show.markSheetManagement.rectify.page.step1" name="rectifyMarkSheetStep1"></forward>
                  <forward path="show.markSheetManagement.rectify.page.step2" name="rectifyMarkSheetStep2"></forward>
                  <forward path="markSheetManagement.viewMarkSheet" name="viewMarkSheet"></forward>
            </action>
            <action scope="request" name="markSheetManagementForm" parameter="method" input="/rectifyMarkSheet.do?page=0&amp;method=validationError" type="net.sourceforge.fenixedu.presentationTier.Action.administrativeOffice.gradeSubmission.MarkSheetCreateDispatchAction" path="/rectifyMarkSheet">
                  <forward path="show.markSheetManagement.rectify.page.step1" name="rectifyMarkSheetStep1"></forward>
                  <forward path="show.markSheetManagement.rectify.page.step2" name="rectifyMarkSheetStep2"></forward>
                  <forward path="/markSheetManagement.do?method=prepareSearchMarkSheet" name="searchMarkSheet"></forward>
                  <forward path="/markSheetManagement.do?method=prepareSearchMarkSheetFilled" name="searchMarkSheetFilled"></forward>
                  <forward path="show.markSheetManagement.showRectificationHistoric" name="showRectificationHistoric"></forward>
            </action>
            <action scope="request" name="printMarkSheetForm" parameter="method" input="/printMarkSheet.do?method=choosePrinterMarkSheet&amp;page=0" type="net.sourceforge.fenixedu.presentationTier.Action.administrativeOffice.gradeSubmission.PrintMarkSheetDispatchAction" path="/printMarkSheet">
                  <forward path="show.printMarkSheet.choosePrinterMarkSheet" name="choosePrinterMarkSheet"></forward>
                  <forward path="/markSheetManagement.do?method=prepareSearchMarkSheetFilled" name="searchMarkSheetFilled"></forward>
                  <forward path="/markSheetManagement.do?method=prepareSearchMarkSheet" name="searchMarkSheet"></forward>
            </action>
            <action scope="request" name="printMarkSheetForm" parameter="method" input="/printMarkSheetWeb.do?method=choosePrinterMarkSheetsWeb&amp;page=0" type="net.sourceforge.fenixedu.presentationTier.Action.administrativeOffice.gradeSubmission.PrintMarkSheetDispatchAction" path="/printMarkSheetWeb">
                  <forward path="show.printMarkSheet.choosePrinterMarkSheetsWeb" name="choosePrinterMarkSheetsWeb"></forward>
                  <forward path="/markSheetManagement.do?method=prepareSearchMarkSheetFilled" name="searchMarkSheetFilled"></forward>
                  <forward path="/markSheetManagement.do?method=prepareSearchMarkSheet" name="searchMarkSheet"></forward>
            </action>
            <!-- ==================== Old MarkSheet Management ================== -->
            <action scope="request" name="printMarkSheetForm" parameter="method" input="/printMarkSheet.do?method=choosePrinterMarkSheet&amp;page=0" type="net.sourceforge.fenixedu.presentationTier.Action.administrativeOffice.gradeSubmission.PrintMarkSheetDispatchAction" path="/printOldMarkSheet">
                  <forward path="show.printMarkSheet.choosePrinterMarkSheet" name="choosePrinterMarkSheet"></forward>
                  <forward path="/oldMarkSheetManagement.do?method=prepareSearchMarkSheetFilled" name="searchMarkSheetFilled"></forward>
                  <forward path="/oldMarkSheetManagement.do?method=prepareSearchMarkSheet" name="searchMarkSheet"></forward>
            </action>
      </action-mappings>
      <!-- ========== Controller Configuration ================================ -->
      <controller processorClass="net.sourceforge.fenixedu.presentationTier.processor.FenixRequestProcessorWithTiles"></controller>
      <!-- ========== Message Resources Definitions ============================ -->
      <message-resources null="false" key="GLOBAL_RESOURCES" parameter="resources.GlobalResources"></message-resources>
      <message-resources null="false" key="APPLICATION_RESOURCES" parameter="resources.ApplicationResources"></message-resources>
      <message-resources null="false" key="ACADEMIC_OFFICE_RESOURCES" parameter="resources.AcademicAdminOffice"></message-resources>
      <message-resources null="false" key="BOLONHA_MANAGER_RESOURCES" parameter="resources.BolonhaManagerResources"></message-resources>
      <message-resources null="false" key="STUDENT_RESOURCES" parameter="resources.StudentResources"></message-resources>
      <message-resources null="false" key="ENUMERATION_RESOURCES" parameter="resources.EnumerationResources"></message-resources>
      <message-resources null="false" key="DEGREE_OFFICE_RESOURCES" parameter="resources.DegreeAdministrativeOfficeResources"></message-resources>
      <message-resources null="false" key="GROUP_NAME_RESOURCES" parameter="resources.GroupNameResources"></message-resources>
      <message-resources null="false" key="COMMON_RESOURCES" parameter="resources.CommonResources"></message-resources>
      <message-resources null="false" key="TITLES_RESOURCES" parameter="resources.TitlesResources"></message-resources>
      <!-- ========== Plug Ins Configuration ================================= -->
      <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
            <set-property value="/WEB-INF/validator-rules.xml,/WEB-INF/conf/validation-alumni.xml" property="pathnames"></set-property>
      </plug-in>
      <plug-in className="pt.ist.fenixWebFramework.struts.tiles.FenixTilesPlugin">
            <set-property value="/WEB-INF/tiles/layouts/tiles-layout-definitions.xml,/WEB-INF/tiles/academicAdminOffice/tiles-academicAdminOffice-definitions.xml,/WEB-INF/tiles/academicAdminOffice/tiles-academicAdminOffice-pages-definitions.xml" property="definitions-config"></set-property>
            <set-property value="true" property="moduleAware"></set-property>
            <set-property value="true" property="definitions-parser-validate"></set-property>
            <set-property value="pt.ist.fenixWebFramework.struts.tiles.FenixDefinitionsFactory" property="factoryClassname"></set-property>
            <set-property value="academicAdminOffice.masterPage" property="defaultTileDefinition"></set-property>
      </plug-in>
      <plug-in className="pt.ist.fenixWebFramework.struts.plugin.StrutsAnnotationsPlugIn"></plug-in>
</struts-config>