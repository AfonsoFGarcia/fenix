<schema  name="import.residence.data" type="net.sourceforge.fenixedu.presentationTier.Action.webSiteManager.SimpleFileBean">
	<slot name="file">
	 	<property name="fileNameSlot" value="name"/>
	    <property name="fileSizeSlot" value="size"/>
		<property name="fileContentTypeSlot" value="contentType"/>
		<property name="size" value="30"/>
	</slot>
</schema>

<schema  name="edit.import.residence.bean" type="net.sourceforge.fenixedu.dataTransferObject.residenceManagement.ImportResidenceEventBean">
	<slot name="residenceYear" label="label.residenceYear" layout="menu-select-postback" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="choiceType" value="net.sourceforge.fenixedu.domain.residence.ResidenceYear" />
		<property name="format" value="${year}"/>
	</slot>
	<slot name="residenceMonth" label="label.residenceMonth" layout="menu-select" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.residenceManagement.ResidenceMonthProvider" />
		<property name="format" value="${month}"/>
		<property name="saveOptions" value="true"/>
	</slot>
	<slot name="paymentLimitDay" label="label.paymentLimitDay"/>
</schema>

	