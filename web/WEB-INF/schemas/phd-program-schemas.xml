<schema name="PhdIndividualProgramProcess.view" type="net.sourceforge.fenixedu.domain.caseHandling.Process" bundle="PHD_RESOURCES">
	<slot name="displayName" key="label.name" />
</schema>

<schema name="PhdProgramCandidacyProcessBean.editProgramInformation" 
		type="net.sourceforge.fenixedu.domain.phd.candidacy.PhdProgramCandidacyProcessBean"
		bundle="PHD_RESOURCES">
	<slot name="program" />
	<slot name="executionYear" />
</schema>

<schema name="PhdProgramCandidacyProcessBean.editPersonalInformation" type="net.sourceforge.fenixedu.dataTransferObject.person.PersonBean" bundle="PHD_RESOURCES">
	<slot name="name" />
	<slot name="gender" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator" />
	<slot name="maritalStatus" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="excludedValues" value="UNKNOWN" />
	</slot>
	<slot name="idDocumentType" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator" />
	<slot name="documentIdNumber" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator" />
	<slot name="documentIdEmissionDate" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="size" value="12"/>
		<property name="maxLength" value="10"/>
	</slot>
	<slot name="documentIdEmissionLocation" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator" />
	<slot name="socialSecurityNumber">
		<property name="size" value="9" />
		<property name="maxLength" value="9" />
	</slot>
	<slot name="dateOfBirth" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="size" value="12"/>
		<property name="maxLength" value="10"/>
	</slot>	
	<slot name="nationality" layout="menu-select" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="choiceType" value="net.sourceforge.fenixedu.domain.Country"/>
		<property name="format" value="${nationality}" />
		<property name="sortBy" value="nationality"/>
	</slot>
	<slot name="fatherName" />
	<slot name="motherName" />
	<slot name="address" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="size" value="50"/>
	</slot>
	<slot name="area" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="size" value="40"/>
	</slot>
	<slot name="areaCode" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="size" value="10"/>
		 <validator class="pt.ist.fenixWebFramework.renderers.validators.RegexpValidator">
            <property name="regexp" value="\d{4}-\d{3}"/>
            <property name="message" value="error.areaCode.invalidFormat"/>
            <property name="key" value="true"/>
        </validator>
	</slot>
	<slot name="areaOfAreaCode" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="size" value="40"/>
	</slot>
	<slot name="phone">
    	<property name="size" value="15"/>
		<property name="maxLength" value="15"/>
		<validator class="pt.ist.fenixWebFramework.renderers.validators.RegexpValidator">
            <property name="regexp" value="(\d{4,15})?"/>
            <property name="message" value="error.phone.invalidFormat"/>
            <property name="key" value="true"/>
        </validator>
    </slot>
    <slot name="workPhone">
    	<property name="size" value="15"/>
		<property name="maxLength" value="15"/>
		<validator class="pt.ist.fenixWebFramework.renderers.validators.RegexpValidator">
            <property name="regexp" value="(\d{4,15})?"/>
            <property name="message" value="error.phone.invalidFormat"/>
            <property name="key" value="true"/>
        </validator>
    </slot>
	<slot name="mobile">
    	<property name="size" value="15"/>
		<property name="maxLength" value="15"/>
		<validator class="pt.ist.fenixWebFramework.renderers.validators.RegexpValidator">
            <property name="regexp" value="(\d{4,15})?"/>
            <property name="message" value="error.phone.invalidFormat"/>
            <property name="key" value="true"/>
        </validator>
    </slot>
	<slot name="email" validator="net.sourceforge.fenixedu.presentationTier.renderers.validators.NotRequiredEmailValidator">
		<property name="size" value="40"/>
	</slot>
</schema>

<schema name="PhdProgramCandidacyProcessBean.editCandidacyInformationBean" type="net.sourceforge.fenixedu.domain.candidacy.CandidacyInformationBean" bundle="APPLICATION_RESOURCES">
	<slot name="countryOfResidence" layout="menu-select" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator"> 
		<property name="format" value="${name}"/>
		<property name="sortBy" value="name=asc" />
		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.DistinctCountriesProvider" />
	</slot>
	<slot name="districtSubdivisionOfResidence" layout="autoComplete">
		<property name="size" value="50"/>
		<property name="labelField" value="name"/>
		<property name="format" value="${name} - (${district.name})"/>
		<property name="indicatorShown" value="true"/>		
		<property name="serviceName" value="SearchDistrictSubdivisions"/>
		<property name="serviceArgs" value="slot=name,size=50"/>
		<property name="className" value="net.sourceforge.fenixedu.domain.DistrictSubdivision"/>
		<property name="minChars" value="1"/>
	</slot>
	<slot name="dislocatedFromPermanentResidence" layout="radio" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="classes" value="dinline liinline nobullet"/>
	</slot>
	<slot name="schoolTimeDistrictSubdivisionOfResidence" layout="autoComplete">
		<property name="size" value="50"/>
		<property name="labelField" value="name"/>
		<property name="format" value="${name} - (${district.name})"/>
		<property name="indicatorShown" value="true"/>		
		<property name="serviceName" value="SearchDistrictSubdivisions"/>
		<property name="serviceArgs" value="slot=name,size=50"/>
		<property name="className" value="net.sourceforge.fenixedu.domain.DistrictSubdivision"/>
		<property name="minChars" value="1"/>
	</slot>
	<slot name="grantOwnerType" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator"/>
	<slot name="numberOfCandidaciesToHigherSchool">
		<property name="size" value="2"/>
		<property name="maxLength" value="2"/>
	</slot>
	<slot name="numberOfFlunksOnHighSchool">
		<property name="size" value="2"/>
		<property name="maxLength" value="2"/>
	</slot>
	<slot name="highSchoolType" layout="menu-select">
		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.candidacy.HighSchoolTypesProvider" />
		<property name="choiceType" value="net.sourceforge.fenixedu.domain.organizationalStructure.AcademicalInstitutionType" />
		<property name="eachLayout" value="this-does-not-exist" />
	</slot>
	<slot name="maritalStatus" key="label.maritalStatus" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="excludedValues" value="UNKNOWN" />
	</slot>
	<slot name="professionType" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator"/>
	<slot name="professionalCondition" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator"/>
	<slot name="motherSchoolLevel" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator" layout="menu-select">
		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.candidacy.SchoolLevelTypeForStudentHouseholdProvider" />
		<property name="choiceType" value="net.sourceforge.fenixedu.domain.SchoolLevelType" />
		<property name="eachLayout" value="this-does-not-exist" />
	</slot>	
	<slot name="motherProfessionType" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator"/>
	<slot name="motherProfessionalCondition" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator"/>
	<slot name="fatherSchoolLevel" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator" layout="menu-select">
		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.candidacy.SchoolLevelTypeForStudentHouseholdProvider" />
		<property name="choiceType" value="net.sourceforge.fenixedu.domain.SchoolLevelType" />
		<property name="eachLayout" value="this-does-not-exist" />
	</slot>	
	<slot name="fatherProfessionType" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator"/>
	<slot name="fatherProfessionalCondition" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator"/>
	<slot name="spouseSchoolLevel" layout="menu-select">
		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.candidacy.SchoolLevelTypeForStudentHouseholdProvider" />
		<property name="choiceType" value="net.sourceforge.fenixedu.domain.SchoolLevelType" />
		<property name="eachLayout" value="this-does-not-exist" />
	</slot>
	<slot name="spouseProfessionType"/>
	<slot name="spouseProfessionalCondition"/>
   	<slot name="institutionUnitName" layout="autoComplete" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator">
		<property name="size" value="50"/>
		<property name="labelField" value="unit.name"/>
		<property name="indicatorShown" value="true"/>
		<property name="serviceName" value="SearchExternalUnits"/>
		<property name="serviceArgs" value="slot=name,size=50"/>
		<property name="className" value="net.sourceforge.fenixedu.domain.organizationalStructure.UnitName"/>
		<property name="minChars" value="1"/>
		<property name="rawSlotName" value="institutionName"/>
	</slot>
	<slot name="countryWhereFinishedPrecedentDegree" layout="menu-select" validator="pt.ist.fenixWebFramework.renderers.validators.RequiredValidator"> 
		<property name="format" value="${name}"/>
		<property name="sortBy" value="name=asc" />
		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.DistinctCountriesProvider" />
	</slot>
	<slot name="schoolLevel" layout="menu-select">
		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.candidacy.SchoolLevelTypeForStudentProvider" />
		<property name="choiceType" value="net.sourceforge.fenixedu.domain.SchoolLevelType" />
		<property name="eachLayout" value="this-does-not-exist" />
	</slot>
	<slot name="otherSchoolLevel" />
</schema>


