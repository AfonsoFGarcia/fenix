<?xml version="1.0" encoding="UTF-8"?>
<!-- @validation.doctype@ -->

<form-validation>
	<formset>
		<!-- Validation for getStudentByNumberAndDegreeTypeForm -->
		<form name="getStudentByNumberAndDegreeTypeForm">
			<field property="degreeType" depends="required,integer">
				<arg0 key="label.choose.degree.type" resource="true"/>
			</field>

			<field property="studentNumber" depends="required,integer">
				<arg0 key="label.choose.student" resource="true"/>
			</field>
		</form>
	</formset>
</form-validation>
