<?xml version="1.0" encoding="UTF-8"?>
<!-- @validation.doctype@ -->

<form-validation>
	<formset>
		<form name="getStudentByNumberAndDegreeTypeForm">
			<field property="degreeType" depends="required,integer">
				<arg0 key="error.no.degree.type.selected" resource="true"/>
			</field>
			<field property="studentNumber" depends="required,integer">
				<arg0 key="error.no.student.selected" resource="true"/>
			</field>
		</form>
	</formset>

	<formset>
		<form name="getDegreeAndCurricularSemesterAndCurricularYearForm">
			<field property="infoExecutionDegreeName" depends="required,integer">
				<arg0 key="error.no.degree.selected" resource="true"/>
			</field>
			<field property="semester" depends="required,integer">
				<arg0 key="error.no.semester.selected" resource="true"/>
			</field>
			<field property="year" depends="required,integer">
				<arg0 key="error.no.year.selected" resource="true"/>
			</field>
		</form>
	</formset>
</form-validation>
