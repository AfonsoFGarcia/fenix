<?xml version="1.0" encoding="UTF-8"?>
<!-- @validation.doctype@ -->

<form-validation>
	<formset>
		<form name="getStudentByNumberAndDegreeTypeForm">
			<field property="degreeType" depends="required,integer">
				<arg0 key="label.choose.degree.type" resource="true"/>
			</field>
			<field property="studentNumber" depends="required,integer">
				<arg0 key="label.choose.student" resource="true"/>
			</field>
		</form>
	</formset>

	<formset>
		<form name="getDegreeAndCurricularSemesterAndCurricularYearForm">
			<field property="infoExecutionDegreeName" depends="required,integer">
				<arg0 key="label.choose.degree" resource="true"/>
			</field>
			<field property="semester" depends="required,integer">
				<arg0 key="label.choose.semester" resource="true"/>
			</field>
			<field property="year" depends="required,integer">
				<arg0 key="label.choose.year" resource="true"/>
			</field>
		</form>
	</formset>
</form-validation>
