<schema name="candidate.precedentDegreeInformation-edit" type="net.sourceforge.fenixedu.dataTransferObject.candidacy.PrecedentDegreeInformationBean" >
	<slot name="degreeDesignation" validator="net.sourceforge.fenixedu.renderers.validators.RequiredValidator" />
	<slot name="conclusionGrade" validator="net.sourceforge.fenixedu.renderers.validators.RequiredValidator" />
	<slot name="conclusionYear" validator="net.sourceforge.fenixedu.renderers.validators.RequiredValidator" />
	<slot name="currentInstitutionName" read-only="true" />
	<slot name="institution" layout="autoComplete" >
		<property name="size" value="40"/>
		<property name="labelField" value="name"/>
		<property name="indicatorShown" value="true"/>		
		<property name="serviceName" value="SearchExternalUnits"/>
		<property name="serviceArgs" value="slot=name"/>
		<property name="className" value="net.sourceforge.fenixedu.domain.organizationalStructure.Unit"/>				
		<property name="minChars" value="4"/>
	</slot>	
	<slot name="newInstitutionName" />	
	<slot name="country" layout="menu-select" key="label.countryOfPrecedenceDegree" validator="net.sourceforge.fenixedu.renderers.validators.RequiredValidator">
		<property name="choiceType" value="net.sourceforge.fenixedu.domain.Country"/>
		<property name="format" value="${name}"/>
	</slot>	
</schema>

