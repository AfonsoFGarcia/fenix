	<slot name="name"/>
	<slot name="gender"/>
	<slot name="documentIdNumber" key="label.identificationNumber"/>
	<slot name="idDocumentType" key="label.idDocumentType"/>
	<slot name="emissionLocationOfDocumentId"/>
