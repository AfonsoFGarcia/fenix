	</slot>
	<slot name="expirationDateOfDocumentIdYearMonthDay">
		<property name="size" value="12"/>
		<property name="maxLength" value="10"/>
	</slot>
	<slot name="socialSecurityNumber" />
	<slot name="profession" />
	<slot name="maritalStatus" />  
</schema>
