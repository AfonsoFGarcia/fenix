<schema name="search.party" type="net.sourceforge.fenixedu.dataTransferObject.parking.SearchPartyBean" bundle="PARKING_RESOURCES">
	<slot name="party" layout="autoComplete" validator="net.sourceforge.fenixedu.renderers.validators.RequiredValidator" key="label.name">
		<property name="size" value="50"/>
		<property name="labelField" value="name"/>
		<property name="format" value="${name}"/>
		<property name="indicatorShown" value="true"/>		
		<property name="serviceName" value="SearchObjects"/>
		<property name="serviceArgs" value="slot=name"/>
		<property name="className" value="net.sourceforge.fenixedu.domain.organizationalStructure.Party"/>
		<property name="minChars" value="3"/>
		<property name="maxCount" value="100"/>		
		<property name="rawSlotName" value="partyName"/>
		<property name="errorStyleClass" value="error0"/>
	</slot>
</schema>