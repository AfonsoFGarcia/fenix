  <schema name="caseHandling.list.processes" type="net.sourceforge.fenixedu.domain.caseHandling.Process">
    <slot name="displayName"/>
  </schema>

  <schema name="caseHandling.list.activities" type="net.sourceforge.fenixedu.caseHandling.Activity">
    <slot name="class.name" layout="slot-as-label">
    	<property name="bundle" value="CASE_HANDLING_RESOURCES" />
    </slot>
  </schema>

  <schema name="caseHandling.testProcess.create.new" type="net.sourceforge.fenixedu.dataTransferObject.caseHandling.TestProcessBean">
	<slot name="degree" key="label.degree"
		layout="menu-select"
		validator="net.sourceforge.fenixedu.renderers.validators.RequiredValidator">
		<property name="providerClass"
			value="net.sourceforge.fenixedu.presentationTier.renderers.providers.DegreesProvider" />
		<property name="choiceType"
			value="net.sourceforge.fenixedu.domain.Degree" />
		<property name="format"
			value="${name}" />
	</slot>
  </schema>
  