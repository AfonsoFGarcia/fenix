<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>

	<!-- MANAGED BEANS -->

	<managed-bean>
		<description>InfoExecutionPeriod</description>
		<managed-bean-name>infoExecutionPeriod</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.dataTransferObject.InfoExecutionPeriod</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<description>name</description>
			<property-name>name</property-name>
			<value>O galo!</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<description>ExecutionPeriods</description>
		<managed-bean-name>executionPeriods</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.example.ExecutionPeriods</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>listStudentThesis</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.coordinator.ListStudentThesis</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>degreeCurricularPlanID</property-name>
			<value>#{param.degreeCurricularPlanID}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>listFirstTimeEnrolmentMasterDegreeStudents</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.gep.ListFirstTimeEnrolmentMasterDegreeStudents</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>	

	<managed-bean>
		<managed-bean-name>updateGratuitySituations</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.manager.gratuity.UpdateGratuitySituations</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>displayCurricularPlan</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.degreeAdministrativeOffice.DisplayCurricularPlan</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>createExecutionDegrees</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.manager.degree.CreateExecutionDegreesForExecutionYear</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	

	<managed-bean>
		<managed-bean-name>createCourseReports</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.manager.executionCourse.CreateCourseReportsForExecutionPeriod</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>	

	<!-- BEGIN: STUDENT Managed Beans -->
	<managed-bean>
		<managed-bean-name>displayEvaluationsToEnrol</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.student.enrolment.DisplayEvaluationsForStudentToEnrol</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<!-- END: STUDENT Managed Beans -->
	

	<!-- BEGIN: TEACHER Managed Beans -->

	<managed-bean>
		<description>EvaluationManagementBackingBean</description>
		<managed-bean-name>evaluationManagementBackingBean</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.teacher.evaluation.EvaluationManagementBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<description>EvaluationManagementMenuBackingBean</description>
		<managed-bean-name>evaluationManagementMenuBackingBean</managed-bean-name>
		<managed-bean-class>net.sourceforge.fenixedu.presentationTier.backBeans.teacher.evaluation.EvaluationManagementMenuBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<!-- END: TEACHER Managed Beans -->
	
	
	<!-- NAVIGATION RULES -->
	
	<navigation-rule>
		<from-view-id>/manager/somePage.jsp</from-view-id>
		<navigation-case>
			<from-outcome>listExecutionPeriods</from-outcome>
			<to-view-id>/manager/somePageListExecutionPeriods.jsp</to-view-id>
		</navigation-case>			
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/degreeAdministrativeOffice/curricularPlans/chooseCurricularPlan.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/degreeAdministrativeOffice/curricularPlans/displayCurricularPlan.jsp</to-view-id>
		</navigation-case>			
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/manager/curricularPlans/chooseCurricularPlan.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/manager/curricularPlans/displayCurricularPlan.jsp</to-view-id>
		</navigation-case>			
	</navigation-rule>	

	<navigation-rule>
		<from-view-id>/manager/degree/chooseDegreeType.jsp</from-view-id>
		<navigation-case>
			<from-outcome>choose</from-outcome>
			<to-view-id>/manager/degree/chooseDegreeCurricularPlans.jsp</to-view-id>
		</navigation-case>			
	</navigation-rule>	
	
	<navigation-rule>
		<from-view-id>/manager/degree/chooseDegreeCurricularPlans.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/manager/degree/insertExecutionDegreesSuccess.jsp</to-view-id>
		</navigation-case>			
	</navigation-rule>

	<!-- BEGIN: STUDENT Navigation Rules -->
	<navigation-rule>
		<from-view-id>/student/enrollment/evaluationsEnrolmentManager.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/student/enrollment/evaluationsEnrolmentManager.jsp</to-view-id>
		</navigation-case>			
	</navigation-rule>
	<!-- END: STUDENT Navigation Rules -->
		
		
	<!-- BEGIN: TEACHER Navigation Rules -->

	<navigation-rule>
		<from-view-id>/teacher/evaluation/writtenTestsIndex.jsp</from-view-id>
 		<navigation-case>
			<from-outcome>enterCreateWrittenTest</from-outcome>
			<to-view-id>/teacher/evaluation/createWrittenTest.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/teacher/evaluation/examsIndex.jsp</from-view-id>
 		<navigation-case>
			<from-outcome>enterEditExamEnrolmentPeriod</from-outcome>
			<to-view-id>/teacher/evaluation/editExamEnrolmentPeriod.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/teacher/evaluation/examsIndex.jsp</from-view-id>
 		<navigation-case>
			<from-outcome>enterShowStudentsEnroled</from-outcome>
			<to-view-id>/teacher/evaluation/showStudentsEnroled.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/teacher/evaluation/showStudentsEnroled.jsp</from-view-id>
 		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/teacher/evaluation/examsIndex.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/teacher/evaluation/createWrittenTest.jsp</from-view-id>
 		<navigation-case>
			<from-outcome>backToWrittenTestsIndex</from-outcome>
			<to-view-id>/teacher/evaluation/writtenTestsIndex.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/teacher/evaluation/examsIndex.jsp</from-view-id>
 		<navigation-case>
			<from-outcome>enterDistributeStudentsByRooms</from-outcome>
			<to-view-id>/teacher/evaluation/distributeStudentsByRooms.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/teacher/evaluation/editExamEnrolmentPeriod.jsp</from-view-id>
 		<navigation-case>
			<from-outcome>backToExamsIndex</from-outcome>
			<to-view-id>/teacher/evaluation/examsIndex.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- END: TEACHER Navigation Rules -->


</faces-config>
