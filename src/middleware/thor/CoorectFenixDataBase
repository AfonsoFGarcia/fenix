create table EXAM_STUDENT (
   ID_INTERNAL int(11) not null auto_increment,
   KEY_EXAM int(11) not null,
   KEY_STUDENT int(11) not null,
   primary key (ID_INTERNAL),
   unique U1 (KEY_EXAM, KEY_STUDENT))
   type=InnoDB;


